{
  "name": "osjs",
  "version": "0.0.0",
  "description": "A (satirical) operating system made in js",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run -s build -- --dev",
    "build:watch": "webpack --watch",
    "build:watch:dev": "npm run -s build:watch -- --dev",
    "build:dev:watch": "npm run -s build:watch:dev",
    "build:start": "npm run -s build && npm run -s start",
    "build:start:dev": "npm run -s build:dev && npm run -s start",
    "start": "node ./dist/backend/backend.bundle.js",
    "start:watch": "nodemon ./dist/backend/backend.bundle.js",
    "start:source": "ts-node ./backend/index.ts",
    "lint": "tslint --project .",
    "typecheck": "tsc --noEmit",
    "test": "npm run -s typecheck && npm run -s lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicholasThrom/osjs.git"
  },
  "author": "Nicholas Throm",
  "license": "UNLICENSED",
  "bugs": {
    "url": "git+https://github.com/NicholasThrom/osjs.git"
  },
  "homepage": "http://127.0.0.1.blockchain.https.osjs.website",
  "dependencies": {
    "@emotion/core": "^10.0.6",
    "@types/color": "^3.0.0",
    "@types/express": "^4.16.0",
    "@types/morgan": "^1.7.35",
    "@types/mz": "0.0.32",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-event-listener": "^0.4.7",
    "@types/react-helmet": "^5.0.8",
    "babel-polyfill": "^6.26.0",
    "color": "^3.1.0",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-event-listener": "^0.6.5",
    "react-helmet": "^5.2.0",
    "socket.io": "^2.2.0",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.6",
    "mz": "^2.7.0",
    "nodemon": "^1.18.9",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0",
    "watch": "^1.0.2",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2",
    "yargs": "^12.0.5"
  }
}
